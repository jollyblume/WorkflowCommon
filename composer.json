{
    "name": "jollyblume/workflow-common",
    "description": "Utility classes and common code",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolly Blume",
            "email": "jollyblume2@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "jollyblume/composed-collections": "@dev",
        "moontoast/math": "^1.1",
        "ramsey/uuid": "^3.8",
        "symfony/event-dispatcher": "^4.2",
        "symfony/property-access": "^4.2",
        "symfony/workflow": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "psr/log": "~1.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "JBJ\\Workflow\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JBJ\\Workflow\\Tests\\": "tests"
        }
    }
}
